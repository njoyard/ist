---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>ist.js</title>
  <link href="http://njoyard.github.com/ist"/>
  <link type="application/atom+xml" rel="self" href="http://njoyard.github.com/ist/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://njoyard.github.com/ist</id>
  <author>
    <name>Nicolas Joyard</name>
    <email>joyard.nicolas@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://njoyard.github.com/ist{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://njoyard.github.com/ist{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Nicolas Joyard</name>
      <uri>http://njoyard.github.com/ist</uri>
    </author>
    <content type="html"><![CDATA[{{ post.content | xml_escape }}]]></content>
  </entry>
  {% endfor %}
 
</feed>
