h3 "@if"
@if trueBoolean
  div "trueBoolean is true"

h3 "@unless"
@unless falseBoolean
  div "falseBoolean is false"

h3 "@with"
@with subContext
  div "subContext.value is {{ opencurly }} value {{ closecurly }}"

h3 "@each"
ol
  @each array
    li
      "Array item #{{ opencurly }} loop.index {{ closecurly }}"
      " is {{ opencurly }} this {{ closecurly }}"

h3 "@eachkey"
dl
  @eachkey object
    dt "{{ opencurly }} key {{ closecurly }}"
    dd "{{ opencurly }} value {{ closecurly }}"